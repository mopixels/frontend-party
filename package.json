{
	"name": "frontend-party",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.5.0",
		"@testing-library/user-event": "^7.2.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-redux": "^7.2.1",
		"react-router-dom": "^5.2.0",
		"react-scripts": "3.4.3",
		"redux": "^4.0.5"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"compile:sass": "node-sass sass/main.scss css/style.comp.css",
		"concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
		"prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.concat.css -o css/stylee.prefix.css",
		"compress:css": "node-sass css/stylee.prefix.css css/style.css --output-style compressed",
		"build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"autoprefixer": "^10.0.1",
		"concat": "^1.0.3",
		"node-sass": "^4.14.1",
		"npm-run-all": "^4.1.5",
		"postcss-cli": "^8.0.0"
	}
}
